var dbFileSource = 'db/Quizz.sqlite';
var dbFileTarget = 'quizz.sqlite';

methods.instalacao = function() {
	alert("oiew");
	var db = Ti.Database.install(dbFileSource, dbFileTarget);
	db.close();
	Ti.API.info('Chamou o install');
}


