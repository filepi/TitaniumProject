exports.consultarUsuarios = function() {
	var db = Ti.Database.open(dbFileTarget);
	var rs = db.execute('select * from users');
	var users = [];
	while (rs.isValidRow()) {
		users.push({
			id:rs.fieldByName('id'),
			nome:rs.fieldByName('nome'),
			email:rs.fieldByName('email')
		});
		rs.next();
	}
	db.close();
	
	return users;
};
